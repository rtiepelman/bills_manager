{
  "name": "bills_manager",
  "version": "1.0.0",
  "description": "Keep track of those bloodthirsty animals that want my money ",
  "main": "index.js",
  "scripts": {
    "serve": "npm run compile | concurrently --kill-others \"npm run compile:w\" \"npm run webpack:w\" \"nodemon index.coffee\"",
    "compile": "coffee -bc . | webpack",
    "compile:w": "coffee -bcw .",
    "webpack:w": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rtiepelman/bills_manager.git"
  },
  "keywords": [
    "bills",
    "manage",
    "bills",
    "nodejs",
    "postgresql",
    "coffeescript"
  ],
  "author": "Bob",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rtiepelman/bills_manager/issues"
  },
  "homepage": "https://github.com/rtiepelman/bills_manager#readme",
  "devDependencies": {
    "coffee-script": "^1.12.2",
    "concurrently": "^3.1.0",
    "express": "^4.14.0",
    "jquery": "^3.1.1",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "express-nunjucks": "^2.1.2",
    "nunjucks": "^3.0.0",
    "pg": "^6.1.2"
  }
}
